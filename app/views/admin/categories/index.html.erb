<h1>Admin Categories</h1>

<%= form_for [:admin, @category] do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.submit %>
<% end %>
<br>
<p> <%= link_to "Your page", user_path(@user) %> </p>
<% @categories.each do |category| %>
  <p><%= category.title %></p>
     <%= link_to "Delete", admin_category_path(category), method: :delete %>
<% end %>
